package app;

import java.util.Scanner;

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    
		Scanner teclado = new Scanner(System.in);
		int num = 0;
		int i;
		
		System.out.println("qual numero");
		num = teclado.nextInt();
		
		for(i=0; i<=10; i++) {
			System.out.println(num + i);
		}
	}

}
