package servico;

import metal.dao.ProdutoDao;
import metal.modelo.Produto;

public class ProdutoServico {
	ProdutoDao produtos = new ProdutoDao();

	public void alterar(Produto produto) {
		return produtos.AlterarProduto(produto);
	}

	
	public boolean excluir(String idProduto) {
		int idUsuario;
		
		idProduto = Integer.parseInt(idProduto);
		
		return produtos.ExcluirProduto(idProduto);
	}
}
}
